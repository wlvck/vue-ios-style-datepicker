{
  "name": "vue-ios-style-datepicker",
  "version": "1.0.2",
  "description": "iOS-style scroll wheel datepicker component for Vue 3",
  "type": "module",
  "main": "./dist/vue-ios-style-datepicker.cjs",
  "module": "./dist/vue-ios-style-datepicker.mjs",
  "unpkg": "./dist/vue-ios-style-datepicker.umd.js",
  "jsdelivr": "./dist/vue-ios-style-datepicker.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-ios-style-datepicker.mjs",
      "require": "./dist/vue-ios-style-datepicker.cjs"
    },
    "./dist/style.css": "./dist/vue-ios-style-datepicker.css",
    "./style.css": "./dist/vue-ios-style-datepicker.css",
    "./css": "./dist/vue-ios-style-datepicker.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build --mode production",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "demo": "npm run build && npx serve .",
    "demo:open": "npm run build && open demo/index.html",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,css}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "prepublishOnly": "npm run lint && npm run build",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlvck/vue3-ios-datepicker.git"
  },
  "keywords": [
    "vue3",
    "vue",
    "datepicker",
    "ios",
    "wheel-picker",
    "scroll-picker",
    "date-selector",
    "time-picker",
    "datetime-picker",
    "typescript",
    "component",
    "ios-style"
  ],
  "author": "Zhumagali Kanagat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wlvck/vue3-ios-datepicker/issues"
  },
  "homepage": "https://github.com/wlvck/vue3-ios-datepicker#readme",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.55.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
